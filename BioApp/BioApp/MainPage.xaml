<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:local="clr-namespace:BioApp"
             x:Class="BioApp.MainPage"
             Title="Drew Ashbacher">

    <StackLayout VerticalOptions="Start">
        <BoxView HeightRequest="80"/>
        <Button BackgroundColor="DarkCyan"
                HeightRequest="60"
                Margin="60,0,60,0"
                Text="About Me"
                x:Name="AboutMeNavigation"
                Clicked="AboutMeNavigation_Clicked"/>
        <BoxView HeightRequest="70"/>
        <Button BackgroundColor="DarkCyan"
                HeightRequest="60"
                Margin="60,0,60,0"
                Text="Skills"
                x:Name="SkillsNavigation"
                Clicked="SkillsNavigation_Clicked"/>
        <BoxView HeightRequest="70"/>
        <Button BackgroundColor="DarkCyan"
                HeightRequest="60"
                Margin="60,0,60,0"
                Text="Contact Me"
                x:Name="ContactMeNavigation"
                Clicked="ContactMeNavigation_Clicked"/>
    </StackLayout>
</ContentPage>
